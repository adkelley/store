<p id="notice"><%= notice %></p>

<h1>Listing Store Its</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Password digest</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store_it| %>
      <tr>
        <td><%= store_it.name %></td>
        <td><%= store_it.email %></td>
        <td><%= store_it.password_digest %></td>
        <td><%= link_to 'Show', store_it %></td>
        <td><%= link_to 'Edit', edit_store_it_path(store_it) %></td>
        <td><%= link_to 'Destroy', store_it, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store it', new_store_it_path %>
